{
  "name": "pfp.blue",
  "version": "1.0.0",
  "description": "Bluesky Profile Picture Management",
  "main": "index.js",
  "scripts": {
    "test": "DEVELOPMENT_MODE=1 playwright test",
    "test:ui": "DEVELOPMENT_MODE=1 playwright test --ui",
    "test:debug": "DEVELOPMENT_MODE=1 playwright test --debug",
    "test:headed": "DEVELOPMENT_MODE=1 playwright test --headed",
    "test:chrome": "DEVELOPMENT_MODE=1 playwright test --project=chromium",
    "test:firefox": "DEVELOPMENT_MODE=1 playwright test --project=firefox",
    "test:safari": "DEVELOPMENT_MODE=1 playwright test --project=webkit",
    "test:fixtures": "USE_FIXTURES=1 DEVELOPMENT_MODE=1 playwright test",
    "test:fixtures:ui": "USE_FIXTURES=1 DEVELOPMENT_MODE=1 playwright test --ui",
    "test:fixtures:headed": "USE_FIXTURES=1 DEVELOPMENT_MODE=1 playwright test --headed",
    "e2e": "./scripts/run-e2e-tests.sh",
    "report": "playwright show-report",
    "report:open": "playwright show-report --browser"
  },
  "keywords": ["bluesky", "profile", "oauth"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.51.0",
    "@types/node": "^22.13.10"
  }
}
